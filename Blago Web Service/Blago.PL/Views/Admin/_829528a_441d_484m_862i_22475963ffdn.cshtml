                                <div class="info-box-content">
                                    <span class="info-box-text">Вывод средств</span>
                                    <span class="info-box-number">@withCnt</span>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-sm-6 col-md-3">
                            <div id="sels" class="info-box mb-3">
                                <span class="info-box-icon  elevation-1" style="background-color:yellow"><i class="fas fa-users"></i></span>

                                <div class="info-box-content">
                                    <span class="info-box-text">Агенты</span>
                                    <span class="info-box-number">@ViewBag.Users.Count </span>
                                </div>
                            </div>
                        </div>

                        <div class="col-12 col-sm-6 col-md-3">
                            <div id="sels" class="info-box mb-3">
                                <span style="background-color:gold" class="info-box-icon  elevation-1">
                                    <i style="color:white" class="far fa-comment light"></i>
                                </span>

                                <div class="info-box-content">
                                    <span class="info-box-text">Сообщения</span>
                                    <span class="info-box-number">@ViewBag.Messages.Count</span>
                                </div>
                                <!-- /.info-box-content -->
                            </div>
                            <!-- /.info-box -->
                        </div>

                        <div class="col-md-12">
                            @if (@ViewBag.IsMsgRes == true)
                            {
                            <h3 style="text-align:center;color:green;background-color:aliceblue">@ViewBag.MsgRes</h3>
                            }
                            <div class="card">
                                @if (ViewBag.IsMenuPart == true)
                                {
                                <div class="card-header">
                                    <h5 class="card-title">
                                        @{
                                                string title = ViewBag.PartialView == "UserInfoPart" ? "Агенты" : ViewBag.PartialView == "BalancePart" ? "Баланс" :
                                              ViewBag.PartialView == "RequestsPart" ? "Вывод средств" : @ViewBag.PartialView == "MessagesPart" ? "Все сообщения" : "";
                                        }
                                        <h2 style="text-align:center;color:gold">@title</h2>
                                    </h5>
                                    <div class="card-tools">
                                        <button type="button" class="btn btn-tool" data-card-widget="collapse">
                                            <i class="fas fa-minus"></i>
                                        </button>
                                        <div class="btn-group">
                                            <button type="button" class="btn btn-tool dropdown-toggle" data-toggle="dropdown">
                                                <i class="fas fa-wrench"></i>
                                            </button>
                                            <div class="dropdown-menu dropdown-menu-right" role="menu">
                                                <a href="#" class="dropdown-item">Action</a>
                                                <a href="#" class="dropdown-item">Another action</a>
                                                <a href="#" class="dropdown-item">Something else here</a>
                                                <a class="dropdown-divider"></a>
                                                <a href="#" class="dropdown-item">Separated link</a>
                                            </div>
                                        </div>
                                        <button type="button" class="btn btn-tool" data-card-widget="remove">
                                            <i class="fas fa-times"></i>
                                        </button>
                                    </div>

                                    <div class="" style="" id="partialsViews">
                                        <!--- Здесь будут подставлятся частичные представления  -->
                                        @if (ViewBag.PartialView == "UserInfoPart")
                                            {
                                        <div class="" style="">
                                            @Html.Partial("~/Views/PartialViews/UserInfoPart.cshtml")
                                        </div>
                                            }
                                            else if (ViewBag.PartialView == "BalancePart")
                                            {
                                        <div style="">
                                            @Html.Partial("~/Views/PartialViews/BalancesPart.cshtml")
                                        </div>
                                            }
                                            else if (ViewBag.PartialView == "RequestsPart")
                                            {
                                        <div style="">
                                            @Html.Partial("~/Views/PartialViews/RequestsPart.cshtml")
                                        </div>
                                            }
                                            else if (ViewBag.PartialView == "MessagesPart")
                                            {
                                        <div style="">
                                            @Html.Partial("~/Views/PartialViews/MessagesPart.cshtml")
                                        </div>
                                            }
                                    </div>
                                </div>
                                }
                            </div>
                            <!-- Main row -->
                            <div class="row">

                                <div class="row">
                                    <div class="col-md-6">

                                    </div>

                                    <div class="col-md-6">

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-4">

                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- /.content -->
        </div>
