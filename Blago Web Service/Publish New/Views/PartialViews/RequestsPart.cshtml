

@{
    var withdraw = ViewBag.Withdrawals as List<Blago.BLL.DTO.WithdrawDto>;
    var users = (List<Blago.PL.Models.ApplicationUser>)ViewBag.Users;

    <table class="table table-dark table-hover">
        <thead>
            <tr style="text-align:center">
                <th scope="col">№</th>
                <th scope="col">Дата</th>
                <th scope="col">ФИО</th>
                <th scope="col">Телефон</th>
                <th scope="col">Номер карты</th>
                <th scope="col">Сумма</th>
                <th scope="col">Статус</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var w in withdraw)
            {
                Blago.PL.Models.ApplicationUser user = users.FirstOrDefault(s => s.UserId == w.UserId);
                <tr style="text-align:center">
                    <td>@w.Id</td>
                    <td>@w.DateWithdraw</td>
                    <td>@user.LastName @user.FirstName[0] . @user.MiddleName[0] .</td>
                    <th scope="row">@user.PhoneNumber</th>
                    <td>@w.NumberCard</td>
                    <td>@w.Amount</td>
                    <td>@w.Status</td>
                </tr>
            }
        </tbody>
    </table>
}

